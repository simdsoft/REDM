<?xml version="1.0"?>
<dm initsize="280,100" minsize="280,100" bresize="1" btranslucent="1" alpha="255">
	<root clrbg="pbgra(3d,3d,3d,ff)">
		<!--root做为唯一的标识,它等同于window Node-->
