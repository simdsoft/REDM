		</window>
		<window pos="0,35,-0,-0" clrbg="pbgra(59,59,59,ff)">
			<label pos="0,5,@40,@22" text="名字:" align="right" font="face:新宋体,size:12,weight:400" clrtext="pbgra(ff,ff,ff,ff)" />
			<limitedit name="ds_name" pos="[0,5,-10,@22" includechars="qwertyuiopasdfghjklzxcvbnm0123456789_-" firstincludechars="qwertyuiopasdfghjklzxcvbnm" clrcaret="pbgra(ff,ff,ff,ff)" skin="ds_attreditframe" caretanimatecount="5" font="face:新宋体,size:12,weight:400" clrtext="rgba(ff,ff,ff,ff)" />
			<button id="1" pos="-70,-32,@60,@22" text="确定" skin="ds_custombutton" font="face:新宋体,size:12,weight:100" clrtext="rgba(ff,ff,ff,ff)" />
		</window>
	</root>
</dm>
